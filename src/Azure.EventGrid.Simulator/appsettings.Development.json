{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "topics": [
    {
      "name": "MyLocalAzureFunctionTopic",
      "port": 7241,
      "key": "TheLocal+DevelopmentKey=",
      "subscribers": [
        {
          "name": "LocalAzureFunctionSubscription",
          "endpoint": "http://localhost:7071/runtime/webhooks/EventGrid?functionName=ExampleFunction",
          "disableValidation": true
        },
        {
          "name": "NexusFileArrivedEvent",
          "endpoint": "http://localhost:7071/runtime/webhooks/EventGrid?functionName=ExampleFunction",
          "disableValidation": false,
          "filter": [
            {
              "includedEventTypes": [ "Collections.BatchProcessor.Nexus.Shared.Events.NexusFileArrivedEvent" ] 
            }
          ] 
        }
      ]
    }
  ]
}
