{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "topics": [
    {
      "name": "MyLocalAzureFunctionTopic",
      "port": 7241,
      "key": "TheLocal+DevelopmentKey=",
      "subscribers": [
        {
          "name": "LocalAzureFunctionSubscription",
          "destination": {
            "endpointType": "WebHook",
            "properties": {
              "endpoint": "http://localhost:7071/runtime/webhooks/EventGrid?functionName=ExampleFunction",
              "disableValidation": true
            }
          },
          "filter": {
            "includedEventTypes": [ "TestEvent" ]
          }
        },
        {
          "name": "MyFileArrivedEvent",
          "destination": {
            "endpointType": "WebHook",
            "properties": {
              "endpoint": "http://localhost:7071/api/MyFileArrived",
              "disableValidation": false
            }
          },
          "filter": {
            "includedEventTypes": [ "Collections.BatchProcessor.My.Shared.Events.MyFileArrivedEvent" ]
          }
        },
        {
          "name": "MyAccountArrivedEvent",
          "destination": {
            "endpointType": "WebHook",
            "properties": {
              "endpoint": "http://localhost:7075/api/MyAccountArrived",
              "disableValidation": false
            }
          },
          "deadLetterDestination": {
            "endpointType": "StorageBlob",
            "properties": {
              "blobContainerName": "eventgrid-deadletter"
            },
            "filter": {
              "includedEventTypes": [ "Collections.BatchProcessor.My.Shared.Events.MyAccountArrivedEvent" ]
            }
          }
        }
      ]
    }
  ]
}
